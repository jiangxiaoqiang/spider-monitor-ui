{
  "name": "spider-monitor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.9.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-preset-react-app": "^7.0.1",
    "eslint": "^5.14.1",
    "eslint-config-react-app": "^3.0.7",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts": "2.1.5"
  },
  "devDependencies": {
    "autoprefixer": "7.1.0",
    "axios": "^0.15.3",
    "browser": "^0.2.6",
    "browserify": "^14.4.0",
    "browserify-shim": "^3.8.14",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "css-loader": "0.28.1",
    "dotenv": "4.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "0.11.1",
    "fs-extra": "3.0.1",
    "gulp": "^3.9.1",
    "history": "^4.6.3",
    "html-webpack-plugin": "3.0.1",
    "immutable": "3.8.1",
    "jest": "20.0.3",
    "material-ui": "^0.20.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "promise": "7.1.1",
    "react-dev-utils": "^3.0.0",
    "react-error-overlay": "^1.0.7",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^3.0.3",
    "redux": "^3.7.1",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^4.3.0",
    "redux-thunk": "^2.2.0",
    "requirejs": "^2.3.3",
    "style-loader": "0.17.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "0.5.8",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "4.4.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "dev": "webpack NODE_ENV=development --display-chunks --watch --progress --colors --display-error-details --config config/webpack.config.dev.js",
    "start": "node scripts/start.js",
    "build": "webpack --progress --profile --colors --config config/webpack.config.prod.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
